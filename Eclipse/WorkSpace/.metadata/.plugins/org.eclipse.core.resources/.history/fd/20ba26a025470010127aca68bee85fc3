package br.com.senaisp.bauru.classes;

public class Estudante {
	//Fields,campos ou variáveis de instância
	private String nome;
	private int ra;
	private double nota;
	private boolean ehFumante;
	//Constructor
	public Estudante() {
		setNome("Semnome");
		ra=0;
		nota=0.0;
		ehFumante=true;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
}
